{
  "name": "GeoBattle",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://www.geoguessr.com/*"],
      "js": ["geoguessr.js"]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.geoguessr.com/*"
  ]
}