{
  "name": "GeoBattle",
  "version": "1.0.0",
  "manifest_version": 2,
  "browser_action": {
  },
  "background": {
    "scripts": [
      "popup-background.js",
      "challenge-background.js",
      "history-background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": ["https://www.geoguessr.com/*"],
      "js": ["geoguessr.js"]
    }
  ],
  "permissions": [
    "tabs",
    "webRequest",
    "storage",
    "https://www.geoguessr.com/*"
  ]
}